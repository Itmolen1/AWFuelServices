    <script>

        function GetOrderNotification() {

            //alert('method caleed');
            ajaxRequest("GET", "/GetMessageses", "", "json").then(function (result) {

                if (result != "Failed") {
                    $('#MassageNotification').text(result);
                }
            });
        }

        $('#NotificationBill').click(function () {
            ajaxRequest("POST", "/CustomerOrderNote", "", "json").then(function (result) {
                if (result != "") {
                    $('#NotificationBody').empty();
                    for (i = 0; i < 5; i++) {
                        $('#NotificationBody').append('<div class="hd-message-info"><a href = "#" ><div class="hd-message-sn"><div class="hd-message-img"><img src="../../img/post/1.jpg" alt="" /></div><div class="hd-mg-ctn"><h3>' + result[i].Company + '</h3><p>' + result[i].statment + '</p></div></div></a ></div >')
                    }
                }
            });
        });


        function GetInfoOnDelivery() {

            ajaxRequest("GET", "/MessageToAdmonOnDelivery", "", "json").then(function (result) {
                if (result != "Failed") {
                    if (result > 0) {
                        $('#DeliverOrderIdNow').val(result);

                        $('#OrderDeliverModelAdmin').modal('show');
                    }
                }
            })
        }


        $('#DeliverdSuccessClose').click(function () {

            Id = $('#DeliverOrderIdNow').val();

            CustomerDeliverdOrderUpdate(Id);
        });


        function CustomerDeliverdOrderUpdate(Id) {
            ajaxRequest("GET", "/CustomerDeliverdOrderUpdate/" + Id, "", "json").then(function (res) {

                if (res != "Failed") {
                    $('#OrderDeliverModelAdmin').modal('hide');
                    AlertCustomerOk();
                }
            });
        }

    </script>
