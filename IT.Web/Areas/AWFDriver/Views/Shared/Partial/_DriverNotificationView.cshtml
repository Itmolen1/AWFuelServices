<div class="modal animated bounce" id="NotifyMeOrder" role="dialog">
    <div class="modal-dialog modal-sm modals-default">
        <div class="modal-content" style="padding:30px 30px">
            <div class="modal-header">
                @*<button type="button" class="close" data-dismiss="modal">&times;</button>*@
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="normal-table-list" style="padding:0 !important">
                            <div class="basic-tb-hd">
                                <h2 style="text-align:center">Order view and Accept</h2>
                            </div>
                            <div class="bsc-tbl-bdr">
                                <table class="table table-bordered">
                                    <tbody>
                                        <tr>
                                            <td>Company</td>
                                            <td><span id="CompanyName" style="font-weight:bold">Dubai Oil</span></td>
                                        </tr>
                                        <tr>
                                            <td>Company Contact</td>
                                            <td id="CompanyContact">091 654564465</td>
                                        </tr>
                                        <tr>
                                            <td>Total Quantity</td>
                                            <td id="Quantity">102 Gellon</td>
                                        </tr>
                                    </tbody>
                                </table>

                                <table id="AsignOrderTable" class="table table-bordered">
